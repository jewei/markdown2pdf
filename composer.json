    "type": "project",
    "require": {
        "dompdf/dompdf": "dev-master",
        "symfony/console": "6.0.x-dev",
        "symfony/filesystem": "6.0.x-dev",
        "league/commonmark": "2.4.x-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jewei Mak",
            "email": "jewei.mak@gmail.com"
        }
